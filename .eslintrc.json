{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-multiple-empty-lines": 0,
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 0,
        "import/extensions": 0,
        "no-param-reassign": 0,
        "no-useless-escape": 0,
        "linebreak-style": 0,
        "import/no-cycle": 0,
        "padded-blocks": 0,
        "require-jsdoc": 0,
        "no-plusplus": 0,
        "no-console": 0,
        "eol-last": 0,
        "max-len": 0,
        "new-cap": 0
    }
}
